{
  "name": "vite-swc-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "React boilerplate with Vite SWC",
  "author": "Hoang Cong Phan <hoangcp219@gmail.com>",
  "keywords": [
    "vite",
    "swc",
    "react",
    "typescript",
    "tailwindcss",
    "eslint",
    "prettier",
    "husky",
    "hygen"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite --port 3000 --open",
    "build": "tsc && vite build",
    "type-check": "tsc --pretty --noEmit",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --write src/**/*.{ts,tsx,css,json} --config ./.prettierrc",
    "gen:component": "hygen components new",
    "gen:page": "hygen pages new",
    "prepare": "husky install"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.11.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/node": "^18.16.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.0",
    "hygen": "^6.2.11",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn prettier --write",
      "yarn eslint --fix"
    ]
  }
}
